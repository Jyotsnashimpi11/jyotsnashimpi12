using System.Linq;
using System.Text;
using System.Windows.Forms;

namespace combobox
{
    public partial class Form1 : Form
    {
        public Form1()
        {
            InitializeComponent();
        }

        private void Cmb_SelectedIndexChanged(object sender, EventArgs e)
        {
            lB1.Items.Clear();
            if (Cmb.SelectedItem == "india")
            {
                lB1.Items.Add("maharashtra");
                lB1.Items.Add("gijrat");
                lB1.Items.Add("usa");
            }

            if (Cmb.SelectedItem =="usa")
            {
                lB1.Items.Add("a");
                lB1.Items.Add("b");
                lB1.Items.Add("u");
            }

            }
